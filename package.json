{
  "name": "openmoth",
  "version": "1.0.0",
  "description": "a twitter account anyone can tweet from",
  "main": "dist/server.js",
  "scripts": {
    "build": "node node_modules/babel-cli/bin/babel.js src -s -D -d dist",
    "dev": "node node_modules/nodemon/bin/nodemon.js -w src -x \"npm run build\"",
    "nodemon": "node node_modules/nodemon/bin/nodemon.js",
    "start": "node dist/server.js",
    "start-dev": "nodemon dist/server.js"
  },
